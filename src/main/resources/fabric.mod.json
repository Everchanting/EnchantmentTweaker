{
  "schemaVersion": 1,
  "id": "enchantmenttweaker",
  "version": "${version}",

  "name": "Enchantment Tweaker",
  "description": "A (slightly hacky) library for changing how enchantments should be applied in minecraft.",
  "authors": [
    "omoflop"
  ],
  "contact": {
    "homepage": "https://github.com/Kingdom-of-Moon/EnchantmentTweaker",
    "sources": "https://github.com/Kingdom-of-Moon/EnchantmentTweaker"
  },
  "license": "CC0-1.0",
  "icon": "assets/enchantmenttweaker/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.moon.enchantmenttweaker.EnchantmentTweakerMod"
    ]
  },
  "mixins": [
    "enchantmenttweaker.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": ">=1.17.x",
    "java": ">=16"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  }
}
